<div class="container px-5 px-sm-0">
  <div class="row">
    <div class="col-md-3">
<%= render 'books/list', user:@user, book:@book %>
</div>
<div class="col-md-8 offset-md-1">

<h2>Users</h2>
<% @users.each do |user| %>
<div>
  <table>
    <thead>
      <th>image</th>
      <th>name</th>
      <th></th>
    </thead>
    <tbody>
      <td><%= image_tag user.get_profile_image %></td>
      <td><%= user.name %></td>
      <td> <%= link_to "Show",user_path(user.id) %></td>
    </tbody>
  </table>
  
 
</div>
<% end %>
</div>
</div>
</div>