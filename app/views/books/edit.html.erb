
<h2>Book Edit</h2>
<%= form_with model: @book do |f| %>
 <label for="inputTitle">Title</label>
  <%= f.text_field :title, autofocus: true, id: "inputTitle", placeholder: "Title"%>
  <label for="inputBody">Body</label>
  <%= f.text_field :body, placeholder: "Body" %>  
  <%= f.submit "変更を保存" %>
<% end %>